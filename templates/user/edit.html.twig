        <div class="title_left">
            <h3>Edit User</h3>
        </div>
    </div>

    <div class="clearfix"></div>
    <div class="row">
        <div class="col-md-12 col-sm-12 ">
            <div class="x_panel">
                <div class="x_title">
                    <h2>User Detail </h2>
                    <div class="clearfix"></div>
                </div>
                <div class="x_content">
                    <div class="row">
                        <div class="col-sm-12">
                            {{ include('user/_form.html.twig', {'button_label': 'Update'}) }}

                        </div>
                    </div>
                </div>
            </div>
        </div>
